<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>银行转账充值</title>
    <meta charset="UTF-8">
    <title>WeUI-pay</title>
    <link rel="stylesheet" href="../../css/jf_cash_account.css">
</head>
<body>
<div id="IframeVip" class="IframeVip" style="display: none">
    <iframe src="../vip/add_card.html" scrolling="auto" id="mainIframeVip" class="mainIframeVip" name="mainIframeVip"></iframe>
</div>

<div class="cash_bank_boot_page">
    <img src="../../images/cash/icon_boot_page.jpg"/>
    <a class="boot_page_button">我知道了</a>
</div>
<div class="vipMain">
    <nav class="Top_navigation">
            <div class="weui_tab_bd"></div>
            <div class="container_tab">
                <a onclick="jfSwitch.goBack()">
                    <div class="nav_left"><img src="../../images/icon_pay_return.png"></div>
                </a>
                <div class="nav_center">
                    <div>银行转账充值</div>
                </div>
                <a href="cash_bank_transfer_instruction.html">
                    <div class="nav_right"><img src="../../images/icon_nav_helpcenter_2.png"></div>
                </a>
            </div>
        </nav>

    <!--选择银行卡-->
    <a class="jd_notice">
        <div class="tab_bd"></div>
        <div class="recharge_balance">
            <img src="../../images/cash/icon_voice.gif">
            <span>
                <p>充值前，必须选择并确定您的付款银行账号</p>
            </span>
        </div>
    </a>
    <div class="cash_bank_transfer_text">选择付款银行</div>
    <div class="cash_choose_pay_bank" id="bank_select" onclick="bankDragUp.run()">
        <a class="choose_pay_bank">
            <div class="pay_bank_text">请选择银行卡</div>
            <div class="pay_bank_arrow"><img src="../../images/cash/icon_pay_bank_arrow.png"/></div>
        </a>
    </div>


    <!--银行卡内容-->
    <section class="main_choose_content">
        <div class="cash_pay_bank_prompt">*仅支持借记卡充值</div>
        <div class="cash_pay_bank_bg">
                <div class="value">
                    <div class="cash_pay_bank_section">收款银行账号</div>

                </div>
                <div class="text_tips">
                    <div class="tip_info">收款账户</div>
                    <div class="tip_number">上海淘略数据处理有限公司</div>
                </div>
                <div class="text_tips">
                    <div class="tip_info">收款账号</div>
                    <div class="tip_number">848797569873</div>
                </div>
                <div class="text_tips">
                    <div class="tip_info">开户行</div>
                    <div class="tip_number">中国建设银行浦东支行</div>
                </div>
        </div>
        <div class="cash_pay_bank_button">
            <button class="vip_nextbtn" id="vip_nextbtn">确认</button>
        </div>
        <div class="cash_pay_bank_service">
            <div class="customer_service" onclick="jfCustomerService.click()"><img src="../../images/icon_dialogue.png">联系客服</div>
        </div>
        <div class="warm_notes">
                <ul>
                    温馨提示
                    <li>首次充值，必须先选择并确定您的付款账号</li>
                    <li>您所选择的银行卡仅限于您本人的工资账户充值</li>
                </ul>
            </div>
    </section>

    <!--银行列表上拉框-->

    <div class="frame_drag_up frame_bank">
        <div class="frame-main">
            <div class="weui_actionsheet_menu">
                <!--弹出菜单-->
                <div class="weui_order">
                    <div class="weui_title">
                        <div>
                            <div>&#xA0;</div>
                            <div>&#x9009;&#x62E9;&#x94F6;&#x884C;&#x5361;</div>
                            <div><img src="../../images/vip/icon-vip-jt.png"></div>
                        </div>
                    </div>
                    <div style="max-height:241px; overflow:auto;">
                        <!--银行卡1-->
                        <div class="weui_bank_imf">
                            <div onclick="bankDragUp.stop()">
                                <div>
                                    <div class="zz">
                                        <div><img src="../../images/vip/icon-bank-1.png" class="bank_logo"></div>
                                        <div>
                                            <div class="bank_name">&#x4E2D;&#x56FD;&#x5EFA;&#x8BBE;&#x94F6;&#x884C;</div>
                                            <div><span class="card_firstnum card_number">622 2654 0231 5648</span><p class="vip_name">&#x8C2F;&#x4E39;</p></div>
                                        </div>
                                    </div>
                                </div>
                                <!--已选择的银行卡-->
                                <div class="weui_choose">&#x5DF2;&#x9009;</div>
                                <div class="weui_cross">&#xD7;</div>
                            </div>
                        </div>
                        <!--银行卡2-->
                        <div class="weui_bank_imf">
                            <div onclick="bankDragUp.stop()">
                                <div>
                                    <div class="zz">
                                        <div><img src="../../images/vip/icon-bank-2.png" class="bank_logo"></div>
                                        <div>
                                            <div class="bank_name">&#x4E2D;&#x56FD;&#x519C;&#x4E1A;&#x94F6;&#x884C;</div>
                                            <div><span class="card_firstnum card_number">622 5632 8597 9955</span><p class="vip_name">&#x8C2F;&#x4E39;</p></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="weui_cross">&#xD7;</div>
                            </div>
                        </div>
                        <!--银行卡3-->
                        <div class="weui_bank_imf">
                            <div onclick="bankDragUp.stop()">
                                <div>
                                    <div class="zz">
                                        <div><img src="../../images/vip/icon-bank-3.png" class="bank_logo"></div>
                                        <div>
                                            <div class="bank_name">&#x4E2D;&#x56FD;&#x5DE5;&#x5546;&#x94F6;&#x884C;</div>
                                            <div><span class="card_firstnum card_number">622 5624 9584 6542</span><p class="vip_name">&#x8C2F;&#x4E39;</p></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="weui_cross">&#xD7;</div>
                            </div>
                        </div>
                        <!--银行卡4-->
                        <div class="weui_bank_imf">
                            <div onclick="bankDragUp.stop()">
                                <div>
                                    <div class="zz">
                                        <div><img src="../../images/vip/icon-bank-4.png" class="bank_logo"></div>
                                        <div>
                                            <div class="bank_name">&#x6D66;&#x4E1C;&#x53D1;&#x5C55;&#x94F6;&#x884C;</div>
                                            <div><span class="card_firstnum card_number">622 6542 6521 0321</span><p class="vip_name">&#x8C2F;&#x4E39;</p></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="weui_cross">&#xD7;</div>
                            </div>
                        </div>
                        <!--银行卡结束-->
                    </div>
                    <!--新卡-->
                    <a href="#">
                        <div class="weui_add_card" id="weui_add_card">
                            <div>
                                <div>&#x4F7F;&#x7528;&#x65B0;&#x5361;</div>
                                <div><img src="../../images/vip/icon_arrow.png"></div>
                            </div>
                        </div>
                    </a></div>

                <!--弹出菜单结束-->
            </div>
        </div>
        <div class="weui_mask_transition mask_drag"></div>
    </div>

</div>




</body>
<script>

    $('.boot_page_button').on("click",function () {

        $('.cash_bank_boot_page').css("display","none")

    })

    //弹出框
    $('.vip_nextbtn').on("click", function (e) {

        var e=event||window.event;

        jfShowTips.dialogShow({
            "mainText": " ",
            "minText": "您只需通过网银向收款账号转账，即可为您的现金账户充值",
            "noCheck": false,
            "noCancel": true,
            "checkBtnText":"知道了",
            "checkFn": function () {
                window.location.href="cash_recharge_page.html"
            }
        });

        e.stopPropagation();
        e.preventDefault()
    })

    //银行列表上拉框
    var bankDragUp=new jfFrameDrag.dragEleshow({

         "targetButton":"bank_select",//点击的目标元素。ID选择器

        "targetDragName":"frame_bank",//出现的目标元素，calss选择器

        "hideButton":"weui_title"//关闭按钮，class选择器

    });


    //删除银行卡
    $('div.weui_cross').on("click", function (e) {

        var thislist = $(this).parents('.weui_bank_imf');

        var e=event||window.event;

        jfShowTips.dialogShow({
            "mainText": "确认删除尾号为8070的银行卡？",
            "minText": " ",
            "checkFn": function () {

                thislist.addClass('payment_list_hide');

                jfShowTips.dialogRemove();//弹出框消失

                setTimeout(function () {
                    thislist.css("display", "none");
                }, 300);

            }
        });

        e.stopPropagation();
        e.preventDefault()
    })


    /*选择新的银行卡*/
    var vipIframeShow=new jfFrameFly.FlyShow({

        "showButton":"weui_add_card",//点击会出现侧拉页面的按钮，ID选择器

        "hideButton":"btn_save",//关闭的按钮,ID选择器

        "flyEleArea":"IframeVip",//侧拉飞入的元素class选择器

        "hasIframe":true,//存在IFrame

        "iframeName":"mainIframeVip",//iframe的class选择器，保持IFrame的name属性与calss值一样

        "InitEleArea":"vipMain"//初始页面，class选择器

    });


    jfCustomerService.init(
            {//用户信息
                name: '阿陈',// 名字
                address: '西伯利亚',// 地址
                email: '1313@qq.com',// 邮箱
                age: '16',// 年龄
                tel: '110',// 电话
                comment: '很帅',// 备注
                gender: '男',// 性别
                qq: '1234567',// QQ
                weibo: '',// 微博
                weixin: '',// 微信
                '公司名称': '上海淘略数据有限公司',
                '公司编号': '123ksuhdaf891274',
                '员工编号': '1234567890123456'
            },
            //指定客服
            'b040d96757bf55ea302f9fbfb0ecd062'
    );
</script>
</html>