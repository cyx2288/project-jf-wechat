<!-- 付款方式-->
<div class="frame_drag_up frame_lawson_pay">
    <div class="frame-main" >
        <div class="public_popup">
            <!--弹出窗口-->
            <div class="weui_actionsheet_menu">
                <div class="title_top">
                    <div class="lawson_title">
                        <p>请选择支付方式</p>
                        <p>优先使用所选支付方式付款，如积分余额不足将尝试使用其他支付方式完成付款</p>
                    </div>

                </div>
                <!--列表-->
                <div class="list_bottom">
                    <!--列表-->
                    <div class="list_cell" onclick="lawsonPayDragUp.stop();">
                        <label class="weui_cell">
                            <div class="weui_cell_bd weui_cell_primary">
                                <img class="pay_icon" src="../../images/mine/icon_mine_welfare.png">
                                <div style="display: inline-block">

                                    <p>福利积分账户</p>
                                    <p>可用余额：100,000,000,000元</p>
                                </div>

                            </div>
                            <div class="weui_cell_hd lawson_selected"></div>
                        </label>

                    </div>
                    <!--列表-->
                    <div class="list_cell" onclick="lawsonPayDragUp.stop();">
                        <label class="weui_cell">
                            <div class="weui_cell_bd weui_cell_primary">
                                <img class="pay_icon" src="../../images/mine/icon_mine_moneybag.png">
                                <div style="display: inline-block">
                                    <p>工资积分余额</p>
                                    <p>可用余额：100,000,000,000元</p>
                                </div>

                            </div>
                            <div class="weui_cell_hd"></div>
                        </label>

                    </div>
                </div>
             </div>

        </div>
    </div>
    <div class="weui_mask_transition mask_drag"></div>
</div>

<script>

    /*点击更多*/

    var lawsonPayDragUp=new jfFrameDrag.dragEleshow({

        "targetButton":"lawsonPay",//点击的目标元素。ID选择器

        "targetDragName":"frame_lawson_pay",//出现的目标元素，calss选择器

        "hideButton":"title_top"//关闭按钮，class选择器

    });

</script>